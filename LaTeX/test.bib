@article{winterl_microbs_2020,
	title = {{micrObs}  {A} customizable time-lapse camera for ecological studies},
	volume = {8},
	issn = {24680672},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2468067220300432},
	doi = {10.1016/j.ohx.2020.e00134},
	abstract = {Camera traps for motion-triggered or continuous time-lapse recordings are readily available on the market. For demanding applications in ecology and environmental sciences, however, commercial systems often lack ﬂexibility to freely adjust recording time intervals, suffer from mechanical component wear, and can be difﬁcult to combine with auxiliary sensors such as GPS, weather stations, or light sensors. We present a robust time-lapse camera system that has been operating continuously since 2013 under the harsh climatic conditions of the Antarctic and Subantarctic regions. Thus far, we have recorded over one million images with individual cameras. The system consumes 122 mW of power in standby mode and captures up to 200,000 high-resolution (16 MPix) images without maintenance such as battery or image memory replacement. It offers time-lapse intervals between 2 s and 1 h, low-light or night-time power saving, and data logging capabilities for additional inputs such as GPS and weather data.},
	language = {en},
	journal = {HardwareX},
	author = {Winterl, Alexander and Richter, Sebastian and Houstin, Aymeric and Nesterova, Anna P. and Bonadonna, Francesco and Schneider, Werner and Fabry, Ben and Le Bohec, Céline and Zitterbart, Daniel P.},
	month = oct,
	year = {2020},
	pages = {e00134},
	file = {Winterl et al. - 2020 - micrObs - A customizable time-lapse camera for eco.pdf:C\:\\Users\\D\\Zotero\\storage\\8QRGJISA\\Winterl et al. - 2020 - micrObs - A customizable time-lapse camera for eco.pdf:application/pdf},
}

